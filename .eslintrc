{
  "env": {
    "browser": true,
    "jest": true,
    "es6": true
  },
  
  "parser": "babel-eslint",
  "plugins": ["import","prettier"],
  "extends": ["eslint:recommended","react-app", "react-app/jest",  "prettier"],
  "parserOptions": {
    
    "ecmaVersion": 2020
   
  
  },
  "rules": {
    "no-console": "warn",
    "no-eval": "error",
    "import/first": "error"
  }
  
}